<?xml version="1.0"?>
<config>
    <production>
        <hd>
            <authentication>
                <encryptionKey>HD Testing Key</encryptionKey>
                <hardTimeout>28800</hardTimeout>
                <inactivityTimeout>3000</inactivityTimeout>
                <requestTimeout>1000000</requestTimeout>
            </authentication>
            <automation>
                <delayDuration>30</delayDuration>
                <monitorLimit>50</monitorLimit>
            </automation>
            <db>
                <params>
                    <database>mysql</database>
                    <dbname>HD_DB_VAL</dbname>
                    <driver_options>
                        <i5_lib>HD_DB_VAL</i5_lib>
                    </driver_options>
                    <username>HD_DB_VAL</username>
                </params>
            </db>
            <general>
                <displayConfirmation>1</displayConfirmation>
                <response_key>data</response_key>
            </general>
            <import>
                <source>HD5.0</source>
                <alternateKeySeparator>}{</alternateKeySeparator>
            </import>
            <jobQueue>HD_PHP_CLI</jobQueue>
            <ldapconfig>
                <ldapuser>cn=admin</ldapuser>
                <host>HOST_VAL</host>
                <port>389</port>
                <basedn>OU=OU_VAL,o=O_VAL,DOMAIN_COMPONENT_STR_VAL</basedn>
                <allowldapmaint>0</allowldapmaint>
            </ldapconfig>
            <bsiconfig>
                <dataset>HDTEST1</dataset>
                <user>HDTEST1</user>
                <version>10</version>
            </bsiconfig>
            <log>
                <error>1</error>
                <login>1</login>
                <automation_schedule>1</automation_schedule>
                <automation_work_queue>0</automation_work_queue>
            </log>
            <search>
                <cache>
                    <life>300</life>
                </cache>
                <limit>
                    <terms>1024</terms>
                </limit>
            </search>
            <service>
                <gateway>http://DOMAIN_VAL:8201</gateway>
                <admin>http://DOMAIN_VAL:8201/admin</admin>
                <attach>http://DOMAIN_VAL:8202</attach>
                <auth>http://DOMAIN_VAL:8203</auth>
                <forms>http://DOMAIN_VAL:8204</forms>
                <importdata>http://DOMAIN_VAL:8207</importdata>
                <md>http://DOMAIN_VAL:8205</md>
                <search>http://DOMAIN_VAL:8206</search>
                <trans>http://DOMAIN_VAL:8205</trans>
                <base_url>/public/</base_url>
                <community>
                    <server>https://appsinhdcommunity.com/</server>
                    <documentation>reference/entity-tables/appsinhdv1/%23</documentation>
                </community>
                <reporting>
                    <cognosServer>http://10.1.0.34:80</cognosServer>
                    <cognosPath>/ibmcognos/cgi-bin/cognos.cgi?b_action=cognosViewer&amp;run.prompt={SELECTION_PROMPT}&amp;run.outputFormat=PDF&amp;ui.action=run&amp;cv.toolbar=false&amp;cv.header=false&amp;ui.object=XSSSTART*2fcontent*2ffolder*5b*40name*3d*27HRIS*27*5d*2ffolder*5b*40name*3d*27HRIS*20VIEWS*27*5d*2freportView*5b*40name*3d*27Report*20View*20of*20{REPORT_NAME}*27*5dXSSEND&amp;ui.name=XSSSTARTReport*20View*20of*20{REPORT_NAME}</cognosPath>
                </reporting>
            </service>
            <Install_info>
                <sslyn>N</sslyn>
                <envpath>/AppsInHD</envpath>
            </Install_info>
            <useAccrueProcessing>1</useAccrueProcessing>
            <useIntercompanyProcessing>1</useIntercompanyProcessing>
            <autoAdjustSpanTransaction>1</autoAdjustSpanTransaction>
        </hd>
    </production>
    <development extends="production" >
        <hd>
            <log>
                <error>1</error>
            </log>
        </hd>
    </development>
</config>
